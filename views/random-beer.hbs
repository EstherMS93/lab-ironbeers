<h1>Random Beer</h1>
  {{#each randomBeer}}
        <div>
            <div>
                <img src="{{this.image_url}}" alt="{{this.name}}">
            </div>

            <div>
                <h1 >{{this.name}}</h1>
                <p >{{this.tagline}}</p>
                <p>{{this.description}}</p>
                <h3>Food pairing</h3>
                <ul>
                    {{#each food_pairing}}
                        <li>{{this}}</li>
                    {{/each}}
                </ul>
                <h3>Brewers Tip</h3>
                <p>{{this.brewers_tips}}</p>
            </div>
        </div>
    {{/each}}